         MACRO
         FINDREQ  &DSECT='YES'
         AIF   (T'&DSECT EQ 'O').DSECT
         AIF   ('&DSECT' EQ 'YES').DSECT
         AIF   ('&DSECT' EQ 'NO').NODSECT
         MNOTE 12,'DSECT MUST BE YES OR NO'
         MEXIT
.DSECT   ANOP
FINDREQ  DSECT
         AGO   .COMM
.NODSECT ANOP
FINDREQ  DS    0H
.COMM    ANOP
FINDNBR  DS    H                     MAX # TO RETURN/# RETURNED
FINDSTAT DS    X
$FINDOK  EQU   X'01'
$FINDNF  EQU   X'02'                 DDNAME NOT FOUND IN TIOT
$FINDOVF EQU   X'03'                 OUTPUT AREA OVERFLOWED
FINDDDNM DS    CL8                   DDNAME
FINDDSNM DS    0CL44                 RETURNED DATASET NAME(S)
         MEND
